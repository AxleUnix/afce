{
    "name": {
        "en_US":"FreeBASIC"
    },
	"algorithm" : {
		"template" : "%branch1%\n"
	},
	"if": {
		"template":"If %cond% Then%branch1%\nElse%branch2%\nEnd If"
	},
	"for": {
		"template":"For %var% = %from% To %to%%branch1%\nNext %var%"
	},
	"pre": {
		"template":"While %cond%%branch1%\nWend"
	},
	"post": {
		"template":"Do%branch1%\nLoop While %cond%"
	},
	"assign": {
		"template":"%dest% = %src%"
	},
	"process": {
		"template": "%text%"
	},
	"io": {
		"template": "%vars%",
		"list": ["vars"],
		"separator": ",",
		"glue": "\n",
		"prefix" : "Input "
	},

	"ou": {
		"template": "%vars%",
		"list": ["vars"],
		"separator": ",",
		"glue": "\n",
		"prefix" : "Print "

	}
}